<p>
	<strong>Title</strong>
	<%= @test.title %>
</p>

<p>
	<strong>Level:</strong>
	<%= @test.level %>
</p>

<p>
	<strong>Category:</strong>
	<%= @test.category.title %>
</p>

<p>
	<strong>Author:</strong>
	<%= @test.author.nickname %>
</p>

<h2>Questions</h2>

<table>
	<tr>
		<th>Title</th>
		<th colspan='3'>Actions</th>
	</tr>
	<%= render partial: 'questions/question', collection: @test.questions %>
</table>

<p>
	<%= link_to 'New question', new_test_question_path(@test.id) %>
</p>

<%= link_to 'Edit', edit_admin_test_path(@test) %>
<%= link_to 'Back', admin_tests_path %>