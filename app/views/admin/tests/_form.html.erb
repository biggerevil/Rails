<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>

  <p>
	<%= form.label :title %><br>
	<%= form.text_field :title %>
  </p><br>

  <p>
	<%= form.label :level %><br>
	<%= form.number_field :level %>
  </p>

  <p>
  	<%= form.label :category_id %>:
  	<%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p>
  	<%= form.label :user_id %>:
  	<%= form.collection_select :user_id, User.all, :id, :nickname, prompt: true %>
  </p>

  <p>
    <%= form.label :time_to_pass_in_minutes %>:
    <%= form.number_field :time_to_pass_in_minutes %>
  </p>

  <p><%= form.submit %></p>
<% end %>